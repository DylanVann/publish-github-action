{
  "name": "publish-github-action",
  "version": "1.1.40",
  "description": "Publish your GitHub Action.",
  "license": "MIT",
  "author": "Dylan Vann <dylan@dylanvann.com> (https://dylanvann.com)",
  "files": [
    "action.yml",
    "dist"
  ],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "yarn run build:ncc && yarn run build:fix",
    "build:fix": "ts-node scripts/build.ts",
    "build:ncc": "ncc build src/index.ts",
    "lint": "tsdx lint",
    "start": "ncc build src/index.ts --watch",
    "test": "tsdx test"
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "devDependencies": {
    "@actions/core": "^1.0.0",
    "@actions/exec": "^1.0.1",
    "@actions/github": "^1.1.0",
    "@octokit/rest": "^16.30.1",
    "@types/fs-extra": "^8.0.1",
    "@types/jest": "^24.0.19",
    "@types/tar": "^4.0.3",
    "@zeit/ncc": "^0.20.5",
    "fs-extra": "^8.1.0",
    "semver": "^6.3.0",
    "tar": "^5.0.5",
    "ts-node": "^8.4.1",
    "tsdx": "^0.10.5",
    "tslib": "^1.10.0",
    "typescript": "^3.6.4"
  }
}
